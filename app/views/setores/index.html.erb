<h1>Lista de  Setores</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setores.each do |setor| %>
      <tr>
        <td><%= setor.nome %></td>
        <td><%= link_to 'Show', setor %></td>
        <td><%= link_to 'Editar', edit_setor_path(setor) %></td>
        <td><%= link_to 'Excluir', setor, method: :delete, data: { confirm: 'VocÃª deseja apagar Registro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

